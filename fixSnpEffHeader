#!/usr/bin/env perl
use Getopt::Long;
use Pod::Usage;
use strict;

my $invcf;
my $outvcf;
my $headvcf;
my $stdin;

GetOptions("i=s"=>\$invcf, "o=s"=>\$outvcf, "h=s"=>\$headvcf, ""=>\$stdin);

my (%meta, $fileformat, $header, @vcf);
my ($IN, $OUT);

if ($stdin) {
    open $IN, '<-';
} elsif ($invcf) {
    open $IN, '<', $invcf or die "$0: Cannot read input vcf '$invcf': $!\n";
} else {
    die "$0: No input specified!\n";
}

if ($outvcf) {
    open $OUT, '>', $outvcf or die "$0: Cannot write output vcf '$outvcf': $!\n";
} else {
    open $OUT, '>-';
}

if ($headvcf) {
    open my $HD, '<', $headvcf or die "$0: Cannot read header vcf '$headvcf': $!\n";
    while (<$HD>) {
	if (/^##fileformat=/) {
	    $fileformat = $_;
	} elsif (/^##([^=]+)/) {
	    push @{ $meta{$1} }, $_;
	} elsif (/^#/) {
	    $header = $_;
	} else {
	    last;
	}
    }
    close $HD;
} else {
    die "$0: No header VCF specified: $!\n";
}

my $printmeta = 0;

while (<$IN>) {
    if (/^##([^=]+)/) {
	push @{ $meta{$1} }, $_;
    } else {
	unless ($printmeta) {
	    ## print combined header before starting on variant lines
	    print $OUT $fileformat;
	    foreach my $key (sort keys %meta) {
		print STDERR "$key\t",scalar(@{ $meta{$key} }),"\n";
		print $OUT $meta{$key}; # foreach sort keys %meta;  # print VCF meta lines in alphabetical order of line type
	    }
	    print $OUT $header;
	    $printmeta = 1;
	}
	die;
	print $OUT $_;
    }
}
close $IN;
close $OUT;
exit;



###### SNPEFF HEADER EXAMPLE

##SnpSiftVersion="SnpSift 4.1a (build 2015-01-14), by Pablo Cingolani"
##SnpSiftCmd="SnpSift varType J476_mut_vs_sib/merged.vcf"
